
logmap:		# c x n
  rlmap
  rot 3 1	# x 0 c
  pop 2
  ret

run:		# n 1
  pop 1		# n
  entc		# n -1
  ipush 15	# n -1 15
  zconv
  rconv
  ipush -2	# n -1 15 -2
  rsh		# n -1 3.75
  ipush 1	# n -1 3.75 1
  zconv
  rconv
  ipush -1	# n -1 3.75 1 -1
  rsh		# n -1 3.75 .5
  dup 4 1	# n -1 3.75 .5 n
  scall logmap	# n -1 x
  dup 2 1	# n -1 x -1
  rapx		# n -1 m e
  lvc 2		# n m e
  ret

main:		# (args)
  texpl		# args n
  jnz run	# 0
  pop 1		#
  ipush 40000	# n
  ipush 1	# n 1
  jmp run
